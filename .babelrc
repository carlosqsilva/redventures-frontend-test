{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "browsers": ["Chrome >= 60", "Firefox >= 55"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-dynamic-import",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
